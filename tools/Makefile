ifndef config
  config=debug
endif
export config

PROJECTS := rover-manager

.PHONY: all clean help $(PROJECTS)

all: $(PROJECTS)

rover-manager: 
	@echo "==== Building rover-manager ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f rover-manager.make

clean:
	@${MAKE} --no-print-directory -C . -f rover-manager.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "   debug"
	@echo "   release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   rover-manager"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"
